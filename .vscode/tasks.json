{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build BandSync iOS App",
			"type": "shell",
			"command": "xcodebuild",
			"args": [
				"-project",
				"BandSync.xcodeproj",
				"-scheme",
				"BandSync",
				"-destination",
				"platform=iOS Simulator,name=iPhone 15",
				"build"
			],
			"group": "build",
			"isBackground": false,
			"problemMatcher": [
				"$xcodebuild"
			]
		}
	]
}